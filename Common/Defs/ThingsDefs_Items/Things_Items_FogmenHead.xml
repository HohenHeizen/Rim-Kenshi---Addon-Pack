<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="ResourceBase">
    <defName>FogmenHead</defName>
    <label>fogmen head</label>
    <description>The chitinous shell of a fogmen's head. Some will pay a hefty some as bounty.</description>
    <graphicData>
      <texPath>Hivers/Fogmen/Fogman</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
      <drawSize>(0.75, 0.75)</drawSize>
    </graphicData>
    <resourceReadoutPriority>Middle</resourceReadoutPriority>
    <stackLimit>25</stackLimit>
    <tradeNeverStack>true</tradeNeverStack>
    <useHitPoints>true</useHitPoints>
    <rotatable>true</rotatable>
    <thingCategories>
      <li>Items</li>
    </thingCategories>
    <statBases>
      <Mass>1</Mass>
      <MaxHitPoints>60</MaxHitPoints>
      <MarketValue>8</MarketValue>
      <DeteriorationRate>4</DeteriorationRate>
    </statBases>
    <comps>
      <li Class="CompProperties_Forbiddable"/>
      <li Class="CompProperties_HasSources"/>
    </comps>
  </ThingDef>

</Defs>